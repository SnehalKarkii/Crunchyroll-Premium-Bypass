{
	"name": "Crunchyroll Premium",
	"version": "1.0.5",
	"description": "Permite ver todas las series de Crunchyroll totalmente gratis.",
	"author": "VirtualOx",
	"icons": { "48": "icon.png" },
	"browser_action":{
		"default_title": "Crunchyroll premium patrocinado por moneyDSBP",
		"default_popup": "Crunchyroll.html",
		"default_icon": "icon.png"
	},
	"permissions": [
		  "storage"
        ],
	"content_scripts": [
    {
        "matches": ["http://www.crunchyroll.com/*", "https://www.crunchyroll.com/*"],
        "css": ["content.css"],
        "js": ["content.js"]
    }
],
	"manifest_version": 2
}